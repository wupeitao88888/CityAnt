<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F4F4"
    android:orientation="vertical">


    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#F4F4F4">
        <!--ptr:ptrAnimationStyle="flip"-->
        <!--ptr:ptrMode="both"-->
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#F4F4F4">

            <LinearLayout
                android:id="@+id/imagesLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/reseive_re"
                    android:layout_width="fill_parent"
                    android:layout_height="72dp"
                    android:background="@android:color/white">


                    <ImageView
                        android:id="@+id/userhead"
                        android:layout_width="53dp"
                        android:layout_height="53dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp" />

                    <TextView
                        android:id="@+id/username"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/userhead"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="9dp"
                        android:layout_toRightOf="@+id/userhead"
                        android:text=""
                        android:textColor="#333333"
                        android:textSize="17dp" />

                    <TextView
                        android:id="@+id/userinfo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/username"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="6dp"
                        android:layout_toRightOf="@+id/userhead"
                        android:text="查看个人主页"
                        android:textColor="#666666"
                        android:textSize="11dp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_alignParentBottom="true"
                        android:background="#F4F4F4" />
                </RelativeLayout>


                <LinearLayout
                    android:id="@+id/wallet_re"
                    android:layout_width="fill_parent"
                    android:layout_height="38dp"
                    android:background="@color/white"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/wallet"
                        style="@style/me_wallet">

                        <ImageView
                            android:id="@+id/income_icon"
                            style="@style/me_wallet_icon"
                            android:src="@drawable/wallet_icon" />

                        <TextView
                            style="@style/forget_new_black"
                            android:layout_toRightOf="@+id/income_icon"
                            android:text="@string/wallet" />


                    </RelativeLayout>
                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"
                        android:background="#F4F4F4" />
                    <RelativeLayout
                        android:id="@+id/Warehouse"
                        style="@style/me_wallet"
                        android:layout_centerHorizontal="true">

                        <TextView
                            style="@style/forget_new_black"
                            android:layout_toRightOf="@+id/warehouse_icon"
                            android:text="@string/warehouse" />

                        <ImageView
                            android:id="@+id/warehouse_icon"
                            style="@style/me_wallet_icon"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="27dp"
                            android:layout_marginStart="27dp"
                            android:src="@drawable/warehouse" />

                    </RelativeLayout>



                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"

                        android:background="#F4F4F4" />

                    <RelativeLayout
                        android:id="@+id/bean"
                        style="@style/me_wallet"
                        android:layout_alignParentRight="true"
                        >

                        <ImageView
                            android:id="@+id/bean_icon"
                            style="@style/me_wallet_icon"
                            android:src="@drawable/bean_icon" />

                        <TextView
                            style="@style/forget_new_black"
                            android:layout_toRightOf="@+id/bean_icon"
                            android:text="@string/bean" />

                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                        android:id="@+id/income_re"
                    android:layout_width="fill_parent"
                    android:layout_height="43dp"
                    android:background="@color/white">

                    <TextView
                        android:id="@+id/today_income"
                        style="@style/forget_new_black"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/todayincome" />

                    <TextView
                        android:id="@+id/income"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:text=""
                        android:textColor="#202020"
                        android:textSize="19dp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="91dp"
                    android:background="@color/white">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_below="@+id/line"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/created"
                            style="@style/me_need">

                            <TextView
                                style="@style/forget_new_black"
                                android:layout_below="@+id/created_icon"
                                android:layout_centerHorizontal="true"
                                android:text="@string/created" />

                            <ImageView
                                android:id="@+id/created_icon"
                                style="@style/me_need_icon"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:background="@drawable/created_icon" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/signup"
                            style="@style/me_need">

                            <ImageView
                                android:id="@+id/signup_icon"
                                style="@style/me_need_icon"
                                android:background="@drawable/signup_icon" />

                            <TextView
                                style="@style/forget_new_black"
                                android:layout_below="@+id/signup_icon"
                                android:layout_centerHorizontal="true"
                                android:text="@string/signup" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/finished"
                            style="@style/me_need">

                            <ImageView
                                android:id="@+id/finished_icon"
                                style="@style/me_need_icon"
                                android:background="@drawable/finished_icon" />

                            <TextView
                                style="@style/forget_new_black"
                                android:layout_below="@+id/finished_icon"
                                android:layout_centerHorizontal="true"
                                android:text="@string/finished" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/judge"
                            style="@style/me_need">

                            <ImageView
                                android:id="@+id/judge_icon"
                                style="@style/me_need_icon"
                                android:background="@drawable/judge_icon" />

                            <TextView
                                style="@style/forget_new_black"
                                android:layout_below="@+id/judge_icon"
                                android:layout_centerHorizontal="true"
                                android:text="@string/judge" />

                        </RelativeLayout>


                    </LinearLayout>

                    <TextView
                        android:id="@+id/title"
                        style="@style/forget_black"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/parttimejob" />

                    <View
                        android:id="@+id/line"
                        style="@style/line"
                        android:layout_below="@+id/title"
                        android:layout_centerHorizontal="true"></View>


                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                    android:id="@+id/me_guess_re"
                    style="@style/me_guess_re">

                    <ImageView
                        android:id="@+id/guess_icon"
                        style="@style/me_guess_icon"
                        android:background="@drawable/guess_icon" />

                    <TextView
                        style="@style/me_guess_text"
                        android:layout_toRightOf="@+id/guess_icon"
                        android:text="@string/guess" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                    android:id="@+id/shopping"
                    style="@style/me_guess_re">

                    <ImageView
                        android:id="@+id/shopping_icon"
                        style="@style/me_guess_icon"
                        android:background="@drawable/shopping_icon" />

                    <TextView
                        style="@style/me_guess_text"
                        android:layout_toRightOf="@+id/shopping_icon"
                        android:text="@string/shopping" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                    android:id="@+id/shports"
                    style="@style/me_guess_re"
                    android:layout_marginTop="1dp"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/activity_icon"
                        style="@style/me_guess_icon" />

                    <TextView
                        style="@style/me_guess_text"
                        android:layout_toRightOf="@+id/activity_icon"
                        android:text="@string/activity" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/center"
                    style="@style/me_guess_re">

                    <ImageView
                        android:id="@+id/center_icon"
                        style="@style/me_guess_icon"
                        android:background="@drawable/center_icon" />

                    <TextView
                        style="@style/me_guess_text"
                        android:layout_toRightOf="@+id/center_icon"
                        android:text="@string/center" />

                    <TextView
                        android:id="@+id/certify"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:text="@string/certify"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/textsize13" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rank"
                    style="@style/me_guess_re">

                    <ImageView
                        android:id="@+id/rank_icon"
                        style="@style/me_guess_icon"
                        android:background="@drawable/rank_icon" />

                    <TextView
                        style="@style/me_guess_text"
                        android:layout_toRightOf="@+id/rank_icon"
                        android:text="@string/rank" />


                    <com.iloomo.widget.CostomRatingBar
                        android:id="@+id/rating_bar_five"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp" />


                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:layout_alignParentBottom="true"
                    android:background="#F4F4F4" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/idea"
                            style="@style/me_guess_re">

                            <ImageView
                                android:id="@+id/idea_icon"
                                style="@style/me_guess_icon"
                                android:background="@drawable/idea" />

                            <TextView
                                style="@style/me_guess_text"
                                android:layout_toRightOf="@+id/idea_icon"
                                android:text="@string/idea" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/service"
                            style="@style/me_guess_re"
                            android:visibility="gone">

                            <ImageView
                                android:id="@+id/service_icon"
                                style="@style/me_guess_icon"
                                android:background="@color/gray" />

                            <TextView
                                style="@style/forget_new_black"
                                android:layout_toRightOf="@+id/service_icon"
                                android:text="@string/service" />

                        </RelativeLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_alignParentBottom="true"
                            android:background="#F4F4F4" />

                        <RelativeLayout
                            android:id="@+id/setting"
                            style="@style/me_guess_re">

                            <ImageView
                                android:id="@+id/setting_icon"
                                style="@style/me_guess_icon"
                                android:background="@drawable/setting_icon" />

                            <TextView
                                style="@style/me_guess_text"
                                android:layout_toRightOf="@+id/setting_icon"
                                android:text="@string/setting" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/undetermined"
                            style="@style/me_need">


                        </RelativeLayout>


                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>

</LinearLayout>
