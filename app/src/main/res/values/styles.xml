<resources>

    <style name="AppTheme" parent="android:Theme.Light">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="icon_re">

        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_marginRight">14dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="img_icon">
        <item name="android:layout_height">16dp</item>
        <item name="android:layout_width">16dp</item>
    </style>

    <style name="login_edittext">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:textColor">#d2d2d2</item>
        <item name="android:textColorHint">#d2d2d2</item>
        <item name="android:textSize">14dp</item>
        <item name="android:background">#00000000</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/login_edittext_height</item>

    </style>

    <style name="fast_login_re">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="fast_login_img_icon">
        <item name="android:layout_width">38.6dp</item>
        <item name="android:layout_height">38.6dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="fast_login_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/fast_login</item>
        <item name="android:textSize">@dimen/textsize13</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>


    <style name="forget_new">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/title_text</item>
        <item name="android:textSize">14dp</item>

    </style>

    <style name="forget_new_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">@dimen/textsize13</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">11dp</item>

    </style>

    <style name="forget_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">@dimen/textsize13</item>
        <item name="android:layout_marginLeft">11dp</item>

    </style>

    <style name="me_wallet">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="me_wallet_icon">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#F4F4F4</item>
    </style>

    <style name="me_need">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="me_need_icon">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="me_guess_re">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="me_guess_icon">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">20dp</item>
    </style>

    <style name="me_guess_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">@color/fast_login</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="me_reseive_re">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="me_reseive_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">@color/black_e1</item>
        <item name="android:textSize">@dimen/textsize13</item>
    </style>

    <style name="me_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/fast_login</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="bank_icon">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>

    </style>

    <style name="wallet_icon">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>


    <style name="wallet_t">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/fast_login</item>
        <item name="android:textSize">17dp</item>

    </style>

    <style name="me_logistics_icon">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="me_logistics_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="myfrends_username">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="myfrends_userhand">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:paddingTop">2dip</item>
        <item name="android:paddingBottom">2dip</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:textColor">@color/btn_gray_normal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/ease_timestampe_bg</item>
    </style>

    <style name="chat_content_date_style">
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_selector</item>
        <item name="android:paddingLeft">10.0dip</item>
        <!--<item name="android:maxHeight">10.0dip</item>-->
    </style>

    <style name="MyDialog_menu" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">@drawable/none_color</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="create_title">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/create_title</item>
    </style>

    <style name="create_check_button">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/create_title</item>
    </style>

    <style name="DialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="addfrend">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="addfrend_icon">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="addfrend_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>

        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13dp</item>
    </style>
    <style name="right_img_style">
        <item name="android:layout_width">13dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/drawableright</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
</resources>
